				{
					"name": "Login - Valid",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status 200 OK\", () => pm.response.to.have.status(200));\r",
									"const ct = pm.response.headers.get(\"Content-Type\") || \"\";\r",
									"pm.test(\"Content-Type is JSON\", () => pm.expect(ct.toLowerCase()).to.include(\"application/json\"));\r",
									"\r",
									"const raw = pm.response.text().trim();\r",
									"pm.test(\"Token returned (non-empty string)\", () => pm.expect(raw.length).to.be.greaterThan(2));\r",
									"\r",
									"const token = raw.replace(/^\"+|\"+$/g, \"\");\r",
									"pm.environment.set(\"token\", token);\r",
									"pm.test(\"Token saved to environment\", () => pm.expect(pm.environment.get(\"token\")).to.be.a(\"string\"));"
								],
