					"name": "Register - Valid",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"if (!pm.environment.get(\"username\")) {\r",
									"  pm.environment.set(\"username\", \"u_\" + Date.now());\r",
									"}\r",
									"if (!pm.environment.get(\"password\")) {\r",
									"  pm.environment.set(\"password\", \"!123Password\");\r",
									"}"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status 201 Created\", () => pm.response.to.have.status(201));\r",
									"\r",
									"const ct = pm.response.headers.get(\"Content-Type\") || \"\";\r",
									"pm.test(\"Content-Type is JSON\", () => pm.expect(ct.toLowerCase()).to.include(\"application/json\"));\r",
									"\r",
									"const json = pm.response.json();\r",
									"pm.test(\"Success message present\", () => pm.expect(json.message).to.eql(\"User created successfully\"));"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
					],
